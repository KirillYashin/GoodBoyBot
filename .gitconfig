[alias]
    s = status --short 
    st = status 
    l = log --oneline --graph --decorate --all 
    g = log --graph --abbrev-commit --decorate --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(dim white)
    - %an%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n %C(white)%s%C(reset)' 
    br = branch 
    co = checkout 
    cm = commit -m  
